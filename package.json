{
  "name": "24-7queue-generator",
  "version": "1.7.5",
  "description": "Allows users to manually generate 24-7queue files for the 24/7 ðŸ”Š discord music bot.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package:mac": "electron-builder --mac",
    "package:win": "electron-builder --win",
    "publish:win": "electron-builder --win -p always"
  },
  "build": {
    "appId": "com.JustWorks.24-7queue-generator",
    "productName": "24-7queue generator",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "extraFiles": [
      "COPYING.txt"
    ],
    "publish": [
      {
        "owner": "JanieUwU",
        "provider": "github",
        "repo": "24-7-Music-Bot-Queue-Generator",
        "private": true
      }
    ],
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "fileAssociations": [
      {
        "ext": "24-7queue",
        "name": "24-7 Queue",
        "description": "24-7QUEUE File"
      }
    ]
  },
  "author": "JustWorks",
  "devDependencies": {
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7"
  },
  "dependencies": {
    "duration-time-format": "^1.1.3",
    "electron-context-menu": "^3.1.1",
    "electron-json-storage": "^4.5.0",
    "electron-log": "^4.4.0",
    "electron-updater": "^4.3.9",
    "ytdl-core": "^4.9.0",
    "ytpl": "^2.2.1"
  }
}
