{
  "name": "24-7queue-generator",
  "version": "1.6.3",
  "description": "Allows users to manually generate 24-7queue files for the 24/7 ðŸ”Š discord music bot.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package:mac": "electron-builder --mac",
    "package:win": "electron-builder --win",
    "publish:win": "electron-builder --win -p always"
  },
  "build": {
    "appId": "com.JustWorks.24-7queue-generator",
    "productName": "24-7queue generator",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "publish": [
      {
        "owner": "JanieUwU",
        "provider": "github",
        "repo": "24-7-Music-Bot-Queue-Generator",
        "private": true
      }
    ],
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "fileAssociations": [
    {
      "name": "24-7 Queue",
      "description": "Queue file for the 24/7 Discord music bot.",
      "ext": [
        "24-7queue"
      ]
    }
  ],
  "author": "JustWorks",
  "devDependencies": {
    "electron": "^11.1.0",
    "electron-builder": "^22.9.1"
  },
  "dependencies": {
    "duration-time-format": "^1.1.3",
    "electron-json-storage": "^4.3.0",
    "electron-log": "^4.3.1",
    "electron-updater": "^4.3.5",
    "ytdl-core": "^4.2.1",
    "ytpl": "^2.0.3"
  }
}
